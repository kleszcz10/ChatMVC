@model List<ChatMVC.Models.ApplicationUser>
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Lista użytkowników<span class="caret"></span></a>
    <ul class="dropdown-menu">
        @foreach(var user in Model)
        {
        <li>
            <a href=@Url.Action("SendMessage","Account",new { @id = user.Id})>@user.Email.ToString().Trim()</a>
        </li>
        }
    </ul>
</li>